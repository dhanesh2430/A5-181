#include <stdio.h>

int main()

{

	int n,a,b,c,i;   	

	// Read the number from user

	printf("Enter an number between 10 to 99: ");

	scanf("%d", &n);

	

	// check whether the number is less than 10 or greater than 99

	if(num > 10 && n < 99)

	{

        a = n/3;

        b = n/3 + 1;

        c = n - (a + b);

        printf("The split numbers are: %d, %d, %d\n", a, b, c);

        printf("The largest split number is: %d \n ", b);

         

     printf("Factors of %d are: \n ", b);

    for (i = 1; i <= b; ++i)

    {

        if (b % i == 0)

        {

            printf(" \n %d ", i);

            

        }

    }    

	}

	

   if(num < 10 || n > 99)

	{

    	printf("Error: Invalid number! please enter a number between 1 to 99\n");

    		

	}

    

    

    

	return 0;

}

// Matrix code
#include<stdio.h>
main()
{
    //Declare the variables.
    int i,j,m,n;
    //Reading the rows and coloumns
    printf("Enter the number of rows:");
    scanf("%d",&m);
    printf("Enter the number of columns:");
    scanf("%d",&n);
    int a[m][n];
    //Declaring the elements.
    for(i=0;i<m;i++)
    {
    	for(j=0;j<n;j++)
    	{
    		printf("Enter the element of a[%d][%d]:",i+1,j+1);
    		scanf("%d",&a[i][j]);
		}
	}
	printf("The matrix:\n");
	//Printing the matrix
	for(i=0;i<m;i++)
    {
    	for(j=0;j<n;j++)
    	{
    		printf("\t%d",a[i][j]);	
	}
		printf("\n");
	}
    return 0;
}
